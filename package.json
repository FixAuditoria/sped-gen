{
  "name": "sped-gen",
  "version": "0.0.0-development",
  "description": "Gerador de código baseado em metadados do SPED",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha",
    "test:watch": "nodemon -w . -d 0 --exec npm test",
    "lint": "eslint .",
    "toc": "doctoc README.md --title **Conteúdo**",
    "prepublish": "npm test && npm run lint",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "engines": {
    "node": ">=4.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elvisgs/sped-gen.git"
  },
  "keywords": [
    "gerador",
    "generator",
    "sped"
  ],
  "author": "Elvis Luciano Guimaraes <elvis.gs@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elvisgs/sped-gen/issues"
  },
  "homepage": "https://github.com/elvisgs/sped-gen#readme",
  "dependencies": {
    "handlebars": "^4.0.5",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "doctoc": "^1.3.0",
    "eslint": "^3.1.1",
    "husky": "^0.14.3",
    "mocha": "^2.5.3",
    "mocha-sinon": "^1.1.5",
    "nodemon": "^1.9.2",
    "rewire": "^2.5.2",
    "semantic-release": "^8.0.3",
    "should": "^10.0.0",
    "should-sinon": "0.0.5",
    "sinon": "^1.17.4"
  }
}
